<%- include('partials/header') -%>
<body class="container">
  <h2 class="mt-5">All Birders</h2>
  <div class="entriesList">
    <% for(let i = 0; i < users.length; i++) { %>
    <div>
      <a href="/entries/user/<%= users[i]._id %>"
        ><img
          class="thumbnail"
          src="<%= users[i].image %>"
          alt="<%= users[i].userName %>"
      /></a>
      <h3 class="text-center mt-1"><%= users[i].userName %></h3>
    </div>
    <% } %>
  </div>
</body>
<%- include('partials/footer') -%>
